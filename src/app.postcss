	--color-warning-300: 255 231 177; /* ⬅ #ffe7b1 */
	--color-warning-400: 255 214 119; /* ⬅ #ffd677 */
	--color-warning-500: 255 196 61; /* ⬅ #FFC43D */
	--color-warning-600: 230 176 55; /* ⬅ #e6b037 */
